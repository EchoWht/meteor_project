 */                                                                                               //
Template.postSubmit.events({                                                                      // 4
    'submit form': function () {                                                                  // 5
        function submitForm(e) {                                                                  // 5
            e.preventDefault();                                                                   // 6
                                                                                                  //
            var post = {                                                                          // 8
                url: $(e.target).find('[name=url]').val(),                                        // 9
                title: $(e.target).find('[name=title]').val()                                     // 10
            };                                                                                    //
                                                                                                  //
            post._id = Posts.insert(post);                                                        // 13
            Router.go('postPage', post);                                                          // 14
        }                                                                                         //
                                                                                                  //
        return submitForm;                                                                        //
    }()                                                                                           //
});                                                                                               //
////////////////////////////////////////////////////////////////////////////////////////////////////

